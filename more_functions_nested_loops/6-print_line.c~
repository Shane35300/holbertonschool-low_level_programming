#include "main.h"

/**
 * more_numbers - print nbre function
 * Description: 10 times the numbers, from 0 to 14
 */
void more_numbers(void)
{
	int time;

	for (time = 0 ; time <= 9 ; time++)
	{
		int nombre;

		for (nombre = 0 ; nombre <= 14 ; nombre++)
		{
			if (nombre >= 10)
			{
				_putchar('0' + (nombre / 10));
			}
			_putchar('0' + (nombre % 10));
		}
		_putchar('\n');
	}
}
